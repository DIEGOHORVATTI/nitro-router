{
  "name": "example-scripts",
  "scripts": {
    "basic": "bun run examples/basic-usage.ts",
    "middleware": "bun run examples/middleware-example.ts",
    "files": "bun run examples/file-upload-example.ts",
    "auth": "bun run examples/auth-example.ts",
    "docs": "bun run examples/documentation-server.ts",
    "all": "concurrently \"npm run basic\" \"npm run middleware\" \"npm run files\" \"npm run auth\" \"npm run docs\"",
    "test:basic": "curl http://localhost:3000/api/users",
    "test:middleware": "curl http://localhost:3001/api/v1/posts",
    "test:files": "curl -H 'Authorization: Bearer valid-token' http://localhost:3002/api/files",
    "test:auth": "curl http://localhost:3003/",
    "test:docs": "curl http://localhost:4000/"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
