{
  "name": "nitro_router",
  "type": "commonjs",
  "description": "Backend for the Scalar project, providing APIs and email services.",
  "license": "MIT",
  "version": "0.1.2",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.3",
    "@types/multer": "2.0.0",
    "@types/node": "24.2.0",
    "@types/swagger-ui-express": "4.1.8",
    "@typescript-eslint/eslint-plugin": "8.39.0",
    "@typescript-eslint/parser": "8.39.0",
    "eslint": "9.32.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-promise": "7.2.1",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "8.1.0",
    "@scalar/api-reference": "1.34.2",
    "@swc/cli": "0.7.8",
    "@swc/core": "1.13.3",
    "cors": "2.8.5",
    "express": "5.1.0",
    "express-rate-limit": "8.0.1",
    "helmet": "8.1.0",
    "http-status-codes": "2.3.0",
    "morgan": "1.10.1",
    "multer": "2.0.2",
    "server": "1.0.42",
    "serverless-http": "3.2.0",
    "swagger-ui-express": "5.0.1",
    "zod": "4.0.15"
  },
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "node dist/src/index.js",
    "build": "swc src -d dist --config-file .swcrc && cp package.json dist/package.json && cp -r src/public dist/src/public",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "update-version": "git add . && git push"
  }
